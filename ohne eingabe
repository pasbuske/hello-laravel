<?php


Route::get('/', function () {

    function foobar()
    {
        for ($i = 1; $i <= 100; $i++) {
            if ($i % 3 === 0 || $i % 5 === 0) {
                if ($i % 3 === 0) {
                    echo('Foo');
                }
                if ($i % 5 === 0) {
                    echo('bar');
                }
            } else {
                echo ($i);
            }
            echo('<br/>');
        }
    }
     
    foobar();


});
?>
